{
  "name": "www",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" webpack --config ./webpack/webpack.config.prod.ts",
    "build:analyze": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" webpack --config ./webpack/webpack.config.analyze.ts",
    "start": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" webpack serve --open --config ./webpack/webpack.config.dev.ts --host=0.0.0.0",
    "start:production": "cross-env ts-node ./index.ts",
    "test": "jest --maxWorkers=50% --detectOpenHandles",
    "test:coverage": "jest --coverage --runInBand",
    "lint": "eslint ./src --ext .ts --ext .tsx",
    "lint:fix": "npm run lint -- --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
