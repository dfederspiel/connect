env:
  browser: true
  es2021: true
  node: true
extends:
  - 'plugin:react/recommended'
  - 'plugin:@typescript-eslint/recommended'
  - 'plugin:prettier/recommended'
parser: '@typescript-eslint/parser'
parserOptions:
  ecmaFeatures:
    jsx: true
  ecmaVersion: 12
  sourceType: module
plugins:
  - prettier
rules: {
  "prettier/prettier": "error",
  "arrow-body-style": "off",
  "prefer-arrow-callback": "off"
}
